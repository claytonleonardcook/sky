    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "^8.0.2",
    "prettier": "^2.8.1",
    "prettier-plugin-svelte": "^3.0.3",
    "sass": "^1.60.0",
    "svelte": "^4.1.1",
    "svelte-check": "^3.4.3",
    "tslib": "^2.4.1",
